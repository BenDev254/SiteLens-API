# Google Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-3
# Optional: API key + CX for Google Custom Search grounding
GOOGLE_API_KEY=your_google_search_api_key
GOOGLE_SEARCH_CX=your_search_engine_id

# CORS Configuration (add production URLs as needed)
CORS_ORIGINS=["http://localhost:5173", "http://localhost:3000"]

# File Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=52428800

# Postregres Database Configuration
DATABASE_URL=postgresql://sitelens_user:YourPassword@localhost:5432/sitelens_db
# Optional runtime flags
MIGRATE_ON_START=false
LOG_LEVEL=INFO
ENVIRONMENT=development

# JWT
SECRET_KEY=replace_this_with_a_secure_random_value
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Google Search & Gemini
GOOGLE_API_KEY=your_google_api_key
GOOGLE_SEARCH_CX=your_search_engine_id
GEMINI_MODEL=gemini-3.0
